<form id="join-class-form" action="/class/join" method="POST">
    <label for="class-code">Class Code</label>
    <input type="text" name="class-code" id="class-code">
    <label for="class-password">Class Password</label>
    <input type="password" name="class-password" id="class-password">
    <button type="submit">Join</button>
</form>
<div id="join-class-error" hidden>
    <ul id="join-class-error-list">

    </ul>
</div>
{{#if error}}
<h3>{{{error}}}</h3>
{{/if}}
<script src="/public/js/scripts/join_class.js"></script>